{
  "name": "react-liquid-gauge",
  "version": "1.2.4",
  "description": "React Liquid Gauge component",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prepublish": "npm run lint && npm test && npm run clean && npm run bowersync && npm run build && npm run build-examples",
    "bowersync": "./scripts/bowersync",
    "build": "babel --out-dir ./lib ./src",
    "build-examples": "cd examples; webpack",
    "clean": "rm -f lib/*",
    "demo": "http-server -p 8000 docs/",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "test": "tap test/*.js --node-arg=--require --node-arg=babel-register --node-arg=--require --node-arg=babel-polyfill",
    "coveralls": "tap test/*.js --coverage --coverage-report=text-lcov --nyc-arg=--require --nyc-arg=babel-register --nyc-arg=--require --nyc-arg=babel-polyfill | coveralls",
    "dev": "cd examples; webpack-dev-server --hot --inline --host 0.0.0.0 --port 8000 --content-base ../docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trendmicro-frontend/react-liquid-gauge.git"
  },
  "author": "Cheton Wu <cheton@gmail.com>",
  "contributors": [
    {
      "name": "Cheton Wu",
      "email": "cheton@gmail.com",
      "url": "https://github.com/cheton"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trendmicro-frontend/react-liquid-gauge/issues"
  },
  "homepage": "https://github.com/trendmicro-frontend/react-liquid-gauge",
  "keywords": [
    "react",
    "react-component",
    "d3",
    "liquid",
    "gauge",
    "chart"
  ],
  "peerDependencies": {
    "react": "^0.14.0 || >=15.0.0",
    "react-dom": "^0.14.0 || >=15.0.0"
  },
  "dependencies": {
    "d3-color": "^1.0.2",
    "d3-ease": "^1.0.2",
    "d3-interpolate": "^1.1.5",
    "d3-scale": "^1.0.6",
    "d3-selection": "^1.1.0",
    "d3-shape": "^1.2.0",
    "d3-timer": "^1.0.3",
    "d3-transition": "^1.1.0",
    "prop-types": "^15.5.8"
  },
  "devDependencies": {
    "@trendmicro/react-buttons": "~1.0.4",
    "babel-cli": "~6.24.0",
    "babel-core": "~6.24.0",
    "babel-eslint": "~7.2.2",
    "babel-loader": "~7.0.0",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-preset-es2015": "~6.24.0",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "coveralls": "~2.13.1",
    "css-loader": "~0.28.4",
    "eslint": "~3.19.0",
    "eslint-config-trendmicro": "~0.5.1",
    "eslint-loader": "~1.7.1",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~2.2.3",
    "eslint-plugin-react": "~6.10.0",
    "file-loader": "~0.11.1",
    "html-webpack-plugin": "~2.28.0",
    "http-server": "~0.10.0",
    "nib": "~1.1.2",
    "react": "^0.14.0 || >=15.0.0",
    "react-dom": "^0.14.0 || >=15.0.0",
    "style-loader": "~0.18.1",
    "stylint": "~1.5.9",
    "stylint-loader": "~1.0.0",
    "stylus-loader": "~3.0.1",
    "tap": "~10.3.0",
    "trendmicro-ui": "~0.4.3",
    "url-loader": "~0.5.8",
    "webpack": "~2.6.1",
    "webpack-dev-server": "~2.4.5",
    "which": "~1.2.12"
  }
}
